{
  "name": "mern-skeleton",
  "version": "1.0.0",
  "description": "mern skeleton application based on Hoque, Shama. Full-Stack React Projects: Modern web development using React 16, Node, Express, and MongoDB.",
  "author": "Jason Mortensen",
  "license": "MIT",
  "keywords": [
    "react",
    "node",
    "express",
    "mongodb",
    "mern"
  ],
  "main": "template.js",
  "scripts": {
    "development": "nodemon",
    "build": "webpack --config webpack.config.client.production.js && webpack --mode=production --config webpack.config.server.js",
    "start": "NODE_ENV=production node ./dist/server.generated.js",
    "pretty": "prettier --single-quote --write \"./*.js\" \"./**/*.js\""
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.8.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "4.16.3",
    "express-jwt": "^5.3.1",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mongodb": "3.0.7",
    "mongoose": "^5.5.2",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-dev-middleware": "3.1.2",
    "webpack-hot-middleware": "2.22.1",
    "webpack-node-externals": "1.7.2"
  }
}
